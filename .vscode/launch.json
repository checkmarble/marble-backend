  "version": "0.2.0",
  "configurations": [
    {
      "name": "Migrate and Launch (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--server",
        "--migrations"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Launch task queue workers (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--worker"
      ],
      "console": "integratedTerminal"
    }
  ]
}
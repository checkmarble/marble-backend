  "version": "0.2.0",
  "configurations": [
    {
      "name": "Migrate and Launch (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--server",
        "--migrations"
      ],
      // "console": "integratedTerminal"
    },
    {
      "name": "Launch scenario scheduling job (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--scheduler"
      ],
      // "console": "integratedTerminal"
    },
    {
      "name": "Launch execute scheduled scenario job (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--scheduled-executer"
      ],
      // "console": "integratedTerminal"
    },
    {
      "name": "Launch data ingestion job (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--data-ingestion"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Launch send pending webhook events job (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--send-pending-webhook-events"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "Launch cron job scheduler (.env)",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/.",
      "envFile": "${workspaceFolder}/.env",
      "args": [
        "--cron-scheduler"
      ],
      "console": "integratedTerminal"
    }
  ]
}
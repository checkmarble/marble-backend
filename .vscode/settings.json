{
      "go.useLanguageServer": true,
      "gopls": {
            "formatting.gofumpt": true
      },
      "go.toolsEnvVars": {
            "GOFUMPT_SPLIT_LONG_LINES": "on"
      },
      "go.lintTool": "golangci-lint",
      "go.lintOnSave": "package",
      "search.exclude": {
            "go.sum": true,
            "tempFiles/*": true,
            "**/*.ndjson": true,
            "LICENSE": true,
            ".git/logs": true,
      },
      "editor.tabSize": 4,
}